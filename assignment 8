#include<stdio.h>
int main()
{
    int arr[100];
    int i,sum=0,n,f=1;
    printf("enter n value\n");
    scanf("%d",&n);
    printf("enter array elemnts-\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        sum=sum+arr[i];
    }
    printf("\nsum= %d \n",sum);
    while(sum>0)
    {
    	f=f*sum;
    	sum--;
	}
	printf("\nfactorial =%d\n",f);
}
